<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.insectmk.stat.mapper.StatMapper">
    <!--查询客户地区-->
    <select id="queryCustomerAreaStat" resultType="cn.insectmk.stat.domain.BaseEntity">
        select address as name,count(1) as value from bus_customer GROUP BY address
    </select>
</mapper>